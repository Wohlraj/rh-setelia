<% provide(:title, "Ajout de skill") %>
<h1><%= yield(:title) %></h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@skill) do |f| %>
		<%= render 'shared/error_messages' %>

		<%= f.label :keyword %>
		<%= f.text_field :keyword %>

		<%= f.submit "Ajouter", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>